        "def draw_test(name, res, input_im, scale, img_row, img_hight):\n",
        "  Black = [0, 0, 0]\n",
        "  res = np.argmax(res, axis=1)\n",
        "  if res[0] == 0:\n",
        "    n_res = 'airplane'\n",
        "  if res[0] == 1:\n",
        "    n_res = 'automobile'\n",
        "  if res[0] == 2:\n",
        "    n_res = 'bird'\n",
        "  if res[0] == 3:\n",
        "    n_res = 'cat'\n",
        "  if res[0] == 4:\n",
        "    n_res = 'deer'\n",
        "  if res[0] == 5:\n",
        "    n_res = 'dog'\n",
        "  if res[0] == 6:\n",
        "    n_res = 'frog'\n",
        "  if res[0] == 7:\n",
        "    n_res = 'horse'\n",
        "  if res[0] == 8:\n",
        "    n_res = 'ship'\n",
        "  if res[0] == 9:\n",
        "    n_res = 'truck'\n",
        "\n",
        "  expanded_image = cv2.copyMakeBorder(input_im, 0, 0, 0,\n",
        "                                    imageL.shape[0]*2, cv2.BORDER_CONSTANT, value=BLACK)\n",
        "\n",
        "  if(color == False):\n",
        "    expanded_image = cv2.cvtColor(expanded_image, cv2.COLOR_GRAY2BGR)\n",
        "\n",
        "  cv2.putText(expanded_image, str(pred), (300, 80),\n",
        "              cv2.FONT_HERSHEY_COMPLEX_SMALL, 3, (0, 255, 0), 2)\n",
        "\n",
        "\n",
        "for i in range(0, 10):\n",
        "  rand = np.random.randint(0, len(x_test))\n",
        "  input_im = x_test[rand]\n",
        "  imageL = cv2.resize(input_im, None, fx=scale, fy=scale,\n",
        "                      interpolation=cv2.INTER_CUBIC)\n",
        "  input_im = input_im.reshape(1, img_row, img_hight, img_depth)\n",
        "\n",
        "  res = str(classifier.predict(input_im, 1, verbose=0)[0])\n",
        "\n",
        "\n",
        "  draw_test = str(classifier.predict(input_im, 1, verbose=0)[0])\n",
        "\n",
        "  cv2.waitKey(0)\n",
        "\n",
        "\n",
